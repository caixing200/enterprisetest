/* pages/exercise/exercise.wxss */
.rootNode {
  box-sizing: border-box;
  padding: 0 60rpx 0 60rpx;
}
.iconfont {
  font-size: 36rpx;
}
.btnBox {
  height: 10%;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center
}
.btn {
  width: 200rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 40rpx;
  font-weight: 500;
}
.btn:nth-child(2) {
  background-color: #04c38e;
  color: #fff;
}
.rootBodys {
  height: 90%;
  box-sizing: border-box;
  padding-top: 100rpx;
}