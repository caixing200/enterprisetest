      <input type="text" placeholder='请填写企业名称' confirm-type='done' bindblur='userInput' maxlength='20'></input>
      <text>*</text>
    </view>
    <view class='btns'>
      <button bindtap='sendResults'>运营效率得分</button>
    </view>
  </view>
</view>