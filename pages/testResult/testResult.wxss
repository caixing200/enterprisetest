/* pages/testResult/testResult.wxss */
.rootNode {
  position: relative
}
.pages {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.results {
  box-sizing: border-box;
  padding: 60rpx 40rpx;
  overflow: hidden;
}
.title {
  margin: 100rpx 0;
  font-weight: 600;
}
.inputs input {
  display: inline-block;
  width: 90%;
  height: 2rem;
  border: 2rpx solid #000;
  vertical-align: middle;
}
.inputs text {
  display: inline-block;
  width: 8%;
  height: 2rem;
  vertical-align: middle;
  box-sizing: border-box;
  padding: 18rpx 0 18rpx 30rpx;
  color: #FF0000;
}
.btns {
  box-sizing: border-box;
  padding: 0 150rpx;
  margin-top: 600rpx;
}
ec-canvas {
  width: 100%;
  height: 100%;
}