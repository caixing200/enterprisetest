.rootNode {
  overflow: hidden;
  background-image: url(http://p9jd05hzv.bkt.clouddn.com/times.png);
  background-repeat: no-repeat;
  background-size: 100% auto;
  background-position-y: 250rpx;
  box-sizing: border-box;
}
.title-style {
  font-size: 70rpx;
  font-weight: 500;
}
.btnStyle {
  font-size: 46rpx;
  font-weight: 800;
}
.footStyle {
  font-weight: 500;
}
.marginB {
  margin-bottom: 26rpx;
}
.marginT {
  margin-top: 200rpx;
}
.titleMargin {
  margin-top: 10%;
  margin-bottom: 70%;
}
.footMargin {
  margin-top: 15%;
}
.btnBox {
  display: inline-block;
  width: 600rpx;
  height: 100rpx;
}
.btnBox button {
  width: 100%;
  height: 100%;
  line-height: 2;
}